[alias]
  co = checkout
  cob = checkout -b
  coo = !git fetch && git checkout
  br = branch
  brd = branch -d
  st = status
  aa = add -A .
  unstage = reset --soft HEAD^
  cm = commit -m
  cma = commit -am
  amend = commit --amend -m
  fix = commit --fixup
  undo = reset HEAD~1
  rv = revert
  cp = cherry-pick
  pu = !git push origin `git branch --show-current`
  fush = push -f
  mg = merge --no-ff
  rb = rebase
  rbc = rebase --continue
  rba = rebase --abort
  rbs = rebase --skip
  rom = !git fetch && git rebase -i origin/master --autosquash
  save = stash push
  pop = stash pop
  apply = stash apply
  rl = reflog

[user]
	email = ravener.anime@gmail.com
	name = Raven Ravener
	signingKey = 7297F61DFFFB3042

[credential]
  helper = store
[core]
	quotepath = false
[commit]
	gpgsig = true
	gpgSign = true
[gpg]
	program = gpg2
[init]
	defaultBranch = main
[pull]
	rebase = true
